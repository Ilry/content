<GameData>
	<BaseGameText>
		<Row Tag="LOC_PARAMETER_FLACS_ISREMIND_NAME">
			<Text>Rest Reminder</Text>
		</Row>
		<Row Tag="LOC_PARAMETER_FLACS_ISREMIND_DESCRIPTION">
			<Text>Enable Rest Reminder?</Text>
		</Row>
		<Row Tag="LOC_PARAMETER_FLACS_TR_MINUTE_NAME">
			<Text>Reminder Time</Text>
		</Row>
		<Row Tag="LOC_PARAMETER_FLACS_TR_MINUTE_DESCRIPTION">
			<Text>When the game lasts for how long (in minutes), player is reminded to take a break.</Text>
		</Row>
		<Row Tag="LOC_FLACS_TIME_REMINDER_TITLE">
			<Text>Rest Reminder</Text>
		</Row>
		<Row Tag="LOC_FLACS_TIME_REMINDER_MESSAGE_1">
			<Text>You have been playing for {1_Minutes} minutes, please take a break.</Text>
		</Row>
		<Row Tag="LOC_FLACS_TIME_REMINDER_MESSAGE_2">
			<Text>You have been playing for {1_Hours} hour(s) and {2_Minutes} minute(s), please take a break.</Text>
		</Row>
		<Row Tag="LOC_FLACS_TIME_REMINDER_OK">
			<Text>I have rested</Text>
		</Row>
		<Row Tag="LOC_FLACS_TIME_REMINDER_NO">
			<Text>One more turn!</Text>
		</Row>
	</BaseGameText>
	
