		<!-- AI接受 -->

		<Replace Tag="LOC_DIPLO_ACCEPT_OPEN_BORDERS_FROM_HUMAN_LEADER_JFD_ISABELLA_ANY" Language="zh_Hans_CN">
			<Text>很好。这是可以接受的。</Text>
		</Replace>

		<!-- AI拒绝 -->

		<Replace Tag="LOC_DIPLO_REJECT_OPEN_BORDERS_FROM_HUMAN_LEADER_JFD_ISABELLA_ANY" Language="zh_Hans_CN">
