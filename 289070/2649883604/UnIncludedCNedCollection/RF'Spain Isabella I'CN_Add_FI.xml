		<!-- AI赞成 -->

		<Replace Tag="LOC_DIPLO_KUDO_EXIT_LEADER_JFD_ISABELLA_ANY" Language="zh_Hans_CN">
			<Text>愿上帝保佑你的财富。</Text>
		</Replace>

