		<Replace Tag="LOC_PEDIA_LEADERS_PAGE_LEADER_MER_YAROSLAV_TITLE" Language="zh_Hans_CN">
			<Text>雅罗斯拉夫</Text>
		</Replace>
		<Replace Tag="LOC_PEDIA_LEADERS_PAGE_LEADER_MER_YAROSLAV_SUBTITLE" Language="zh_Hans_CN">
			<Text>基辅和诺夫哥罗德大公</Text>
		</Replace>
